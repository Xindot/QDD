@import "../../template/tripList/index.wxss";

.me-list-one {
  text-align: center;
  background-color:#ffffff;
  font-size:14px;
  color:#666;
  margin-top:10px;
  padding: 12px;
}
.contact-2 {
  text-align: center;
  background-color:#ffffff;
  font-size:14px;
  color:#666;
  margin-top:10px;
}
.contact-2.empty {
  padding: 12px;
}
.contact-one {
  width: 50%;
  display: inline-block;
  box-sizing: border-box;
  vertical-align: top;
  padding: 5px;
}
.contact-one + .contact-one {
  border-left: dashed 1px #efefef;
}


.trip-set {
  text-align: center;
  background-color:#ffffff;
  font-size:14px;
  padding: 12px;
  color:#666;
}

.trip-o-2 {
  border-top: dashed 1px #efefef;
  text-align: center;
  background-color:#ffffff;
  font-size:14px;
  color:#666;
}
.trip-o-one {
  width: 50%;
  display: inline-block;
  box-sizing: border-box;
  padding: 12px 0;
}
.trip-o-one + .trip-o-one {
  border-left: dashed 1px #efefef;
}


.Version {
  margin-top: 10px;
  text-align:center;
  width:100%;
  color:#666;
  font-size:14px;
}