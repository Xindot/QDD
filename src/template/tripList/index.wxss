/* pubList start */
.pub-one {
  background-color:#fff;
  padding:5px;
  position: relative;
  overflow: hidden;
}
.pub-one + .pub-one {
  border-top: dashed 1px #efefef;
}

.point-one {
  display:inline-block;
  width:40%;
  text-align:center;
  vertical-align:top;
  box-sizing:border-box;
}
.point-go {
  display:inline-block;
  width:20%;
  text-align:center;
  vertical-align:top;
  box-sizing:border-box;
}

.status-0 {
  position: absolute;
  left: -30px;
  top: -30px;
  width: 60px;
  height: 60px;
  background-color: #999;
  transform:rotate(-45deg);
  -ms-transform:rotate(-45deg);
  -moz-transform:rotate(-45deg);
  -webkit-transform:rotate(-45deg);
  -o-transform:rotate(-45deg);
}
.status-0 > view {
  text-align: center;
  color: #fff;
  font-size: 10px;
  margin-top: 45px;
}
/* pubList end */
