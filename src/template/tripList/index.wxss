/* pubList start */
.pub-one {
  background-color:#fff;
  padding:5px;
}
.pub-one + .pub-one {
  border-top: dashed 1px #efefef;
}

.point-one {
  display:inline-block;
  width:40%;
  text-align:center;
  vertical-align:top;
  box-sizing:border-box;
}
.point-go {
  display:inline-block;
  width:20%;
  text-align:center;
  vertical-align:top;
  box-sizing:border-box;
}
/* pubList end */
